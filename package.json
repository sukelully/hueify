{
  "name": "hueify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 127.0.0.1",
    "dev:v": "vercel dev --listen 127.0.0.1:3000",
    "build": "next build",
    "build:t": "next build --turbopack",
    "build:log": "next build > build-log.txt 2>&1",
    "start": "next start",
    "lint": "eslint",
    "prettier": "prettier . --write",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@types/chroma-js": "^3.1.1",
    "@vibrant/types": "^4.0.0",
    "better-auth": "^1.3.9",
    "chroma-js": "^3.1.2",
    "next": "15.5.0",
    "node-vibrant": "^4.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "colorthief": "^2.6.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.16.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
